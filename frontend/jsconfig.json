{
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"]
    },
    "baseUrl": ".",
    "checkJs": false,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    ".next",
    "out"
  ],
  "include": [
    "**/*.js",
    "**/*.jsx"
  ]
}
